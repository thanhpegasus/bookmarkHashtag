{
    "name": "Bookmark Hashtag",
    "description": "Bookmark Hashtag",
    "version": "0.1",
    "manifest_version": 2,

    "permissions": [
        "bookmarks"
    ],
    
    "app": {
        "launch": {
            "local_path": "./app/index.html"
        }
    },
    
    "icons": { 
        "16": "content/images/icon.png", 
        "128": "content/images/icon.png" },
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'"
}